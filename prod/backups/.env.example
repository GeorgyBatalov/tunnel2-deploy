# PostgreSQL Backup Configuration
# Copy this file to .env and fill in the values

# PostgreSQL credentials
POSTGRES_USER=tunnel
POSTGRES_BACKUP_USER=backup_user
POSTGRES_BACKUP_PASSWORD=CHANGE_ME_GENERATE_WITH_openssl_rand_-base64_32

# Yandex.Disk credentials (for rclone WebDAV)
# Get app password from: https://id.yandex.ru/security/app-passwords
YANDEX_USERNAME=your_yandex_username
YANDEX_APP_PASSWORD=CHANGE_ME_YANDEX_APP_PASSWORD
